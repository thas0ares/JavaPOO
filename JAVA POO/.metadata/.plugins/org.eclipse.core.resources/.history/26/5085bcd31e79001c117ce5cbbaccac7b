package reforcoaula02;

public class Internacao {
	String local;
	int totalLeitos;
	int leitoOcupado;
	int leitoVago;

	void status() {
		System.out.println("Qual unidade? " + this.local);
		System.out.println("Quantos leitos existem nessa unidade? " + this.totalLeitos);
		System.out.println("Quantos leitos estão ocupados?  " + this.leitoOcupado);
		System.out.println("Quantos leitos estão vagos?" + this.leitoVago);
	}
}
